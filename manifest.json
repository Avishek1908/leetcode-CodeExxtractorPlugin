{
  "manifest_version": 3,
  "name": "LeetCode Extractor",
  "version": "1.0",
  "description": "Extracts your LeetCode solution code.",
  "permissions": ["scripting", "activeTab"],
  "action": {
    "default_popup": "popup.html"
  },
  "content_scripts": [
  {
    "matches": ["*://leetcode.com/*"],
    "js": ["content.js"],
    "run_at": "document_idle"
  }
],

  "host_permissions": ["*://leetcode.com/*"]
}
